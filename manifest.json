{
  "name": "Instagram Downloader",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Injects download buttons to Instagram Media",
  "homepage_url": "http://aashutoshrathi.co",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "icons": {
    "16": "img/icon-16x16.png",
    "32": "img/icon-32x32.png",
    "128": "img/icon-128x128.png"
  },

  "browser_action": {
    "default_title": "Instagram Downloader",
    "default_popup": "popup.html",
    "default_icon": "img/icon-16x16.png"
  },

  "web_accessible_resources": ["insta-down-css.css"],

  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "https://www.instagram.com/"
  ]
}
